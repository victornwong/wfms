	if(qtrows.size() > 5) newlb.setRows(20);
	newlb.addEventListener("onSelect", new quotes_lb_Listener());
	for(dpi : qtrows)
	{
		ArrayList kabom = new ArrayList();
		origid = dpi.get("origid").toString();
		kabom.add(origid);
