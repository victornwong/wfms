		kabom.add(filename);

		strarray = kiboo.convertArrayListToStringArray(kabom);
		lbhand.insertListItems(newlb,strarray,"false","");
	}
	
	ds_sql.close();

	senthistory_popup.open(senthistory_btn);
}

